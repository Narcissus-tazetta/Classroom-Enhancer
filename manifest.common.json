{
    "manifest_version": 3,
    "name": "Classroom Enhancer",
    "version": "1.0.2",
    "description": "Google ClassroomのUIの改善します",
    "content_scripts": [
        {
            "matches": ["https://classroom.google.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": ["https://classroom.google.com/*"],
    "icons": {
        "128": "icons/mop.png"
    }
}
